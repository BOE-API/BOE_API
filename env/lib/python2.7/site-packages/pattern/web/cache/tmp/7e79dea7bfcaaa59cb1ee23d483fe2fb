        <anterior referencia="BOE-A-2009-3025" orden="3060">
          <palabra codigo="440">DE CONFORMIDAD con</palabra>
          <texto>el art. 6.2 del REAL DECRETO 170/2009, de 13 de febrero</texto>
        </anterior>
